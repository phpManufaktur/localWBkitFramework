{% extends getTemplateFile('@phpManufaktur/Basic/Template', 'framework/body.twig') %}
{% block stylesheet %}
    {{ parent() }}
  <link rel="stylesheet" type="text/css" href="{{ MANUFAKTUR_URL }}/Contact/Template/default/backend/simple/css/contact.css" media="screen, projection" />
  <link rel="stylesheet" type="text/css" href="{{ MANUFAKTUR_URL }}/Contact/Template/default/backend/simple/css/company.contact.css" media="screen, projection" />
{% endblock %}

{% block content %}
  {% if message|length > 0 %}
    <div class="message">{{ message }}</div>
  {% endif %}
  {% include '@phpManufaktur/Contact/Template/default/backend/form/edit.company.contact.twig' %}
{% endblock content %}
