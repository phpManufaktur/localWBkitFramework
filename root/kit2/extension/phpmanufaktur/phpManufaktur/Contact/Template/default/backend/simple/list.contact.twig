{% extends getTemplateFile('@phpManufaktur/Basic/Template', 'framework/body.twig') %}
{% block stylesheet %}
    {{ parent() }}
  <link rel="stylesheet" type="text/css" href="{{ MANUFAKTUR_URL }}/Contact/Template/default/backend/simple/css/list.css" media="screen, projection" />
  <link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet">
{% endblock %}

{% block content %}
  {% if message|length > 0 %}
    <div class="message">{{ message }}</div>
  {% endif %}

  {% include '@phpManufaktur/Contact/Template/default/backend/form/list.contact.twig' %}

{% endblock content %}
